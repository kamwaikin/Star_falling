package {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.net.*;	public class End extends TScene {			var name1 : String;			var score : int;			var url:String;		public function End(main: TMain, name1: String, score: int) {			mMain = main;			EndName.text="Your name : "+name1;			EndScore.text="Your score : "+String(score);			this.name1=name1;			this.score=score;			restart.addEventListener(MouseEvent.MOUSE_DOWN, OnButtonDown);		}		protected function OnButtonDown(e: MouseEvent): void {			//while (stage.numChildren > 1) 			//stage.removeChildAt(0);			url="http://default-environment.gmpywmwi2j.ap-southeast-1.elasticbeanstalk.com/Untitled-3.php"+"?score="+score+"&name='"+name1+"'";			new URLLoader(new URLRequest(url));			var scene: Start = new Start(this.mMain);			//this.addChild(scene);			this.mMain.SetCurrentScene(scene);		}		override public function Release(): void {			super.Release();			// release extra stuffs			restart.removeEventListener(MouseEvent.MOUSE_DOWN, OnButtonDown);		}	}}