package{	import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.net.*;	import flash.events.Event;		public class Start extends TScene	{		public function Start(main:TMain):void		{			mMain = main;			btnStart.addEventListener(MouseEvent.MOUSE_DOWN,OnButtonDown);			btnRank.addEventListener(MouseEvent.MOUSE_DOWN,OnButtonDown1);		}		protected function OnButtonDown(e:MouseEvent):void		{			if( InputName.text.split(" ").join("") != ""){				var scene:GameplayScene=new GameplayScene(this.mMain, InputName.text.split(" ").join(""));				this.mMain.SetCurrentScene(scene);				}			else 				ErrorText.text="The name must not only contain space.";		}				protected function OnButtonDown1(e:MouseEvent):void		{			var scene:ranking=new ranking(this.mMain);			this.mMain.SetCurrentScene(scene);		}		/*override public function Release():void		{			super.Release();						// release extra stuffs			startButton.removeEventListener(MouseEvent.MOUSE_DOWN,OnButtonDown);		}*/	}}