package{	import flash.display.MovieClip;	import flash.events.KeyboardEvent;		public class TMain extends MovieClip	{		var mCurrentScene:TScene = null;		public var keyMap:Array=new Array();				public function TMain():void		{			var sc:Start=new Start(this);			this.SetCurrentScene(sc);						//stage.addEventListener(KeyboardEvent.KEY_DOWN,OnKeyDown);			//stage.addEventListener(KeyboardEvent.KEY_UP,OnKeyUp);						//this.keyMap['LEFT']=false;			//this.keyMap['RIGHT']=false;		}				/*protected function OnKeyDown(e:KeyboardEvent):void		{			if(e.keyCode ==37)				this.keyMap['LEFT']=true;			else if (e.keyCode==39)				this.keyMap['RIGHT']=true;		}				protected function OnKeyUp(e:KeyboardEvent):void		{			if(e.keyCode ==37)				this.keyMap['LEFT']=false;			else if (e.keyCode==39)				this.keyMap['RIGHT']=false;		}*/				public function SetCurrentScene(scene:TScene):void		{			if(mCurrentScene)			{				mCurrentScene.Release();				this.removeChild(mCurrentScene);							}						this.addChild(scene);			mCurrentScene = scene;		}	}}