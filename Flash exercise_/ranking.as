package  {	import flash.net.*;	import flash.events.Event;	import flash.text.*;		import flash.events.MouseEvent;	public class ranking extends TScene{		public function ranking(main: TMain) {			mMain = main;			btnMain.addEventListener(MouseEvent.MOUSE_DOWN,OnButtonDown);			var tf:TextFormat = new TextFormat();  			tf.size = 25; 			tf.font = "Arial";			rankTable.setStyle("textFormat", tf); 			var url: URLRequest = new URLRequest("http://default-environment.gmpywmwi2j.ap-southeast-1.elasticbeanstalk.com/Untitled-3.php?score=-99&name='a'")			var x :URLLoader = new URLLoader(url);  			x.addEventListener(Event.COMPLETE, onLoadData);		}		public function onLoadData(e:Event):void {			var a:String=e.target.data;			trace(a);			var b:String="";			var result:Array;			result=a.split("name: ");			trace(result[0]+"    "+result[1]);			for(var i:int = 1;i<11;i++)				b+="Name: "+result[i]+"\n";			rankTable.text=b;		}				protected function OnButtonDown(e:MouseEvent):void		{			var scene: Start = new Start(this.mMain);			this.mMain.SetCurrentScene(scene);		}	}	}