package {	import flash.display.MovieClip;	import flash.events.TimerEvent;	import flash.events.Event;	import flash.utils.getTimer;	import flash.utils.Timer;	import flash.geom.ColorTransform;		public class GameplayScene extends TScene {		var score: int = 0;		var previousTime: int = 0;		var array: Array = new Array();		var mCurrentScene: TScene = null;		var name1: String;				public function GameplayScene(main: TMain, name: String): void {			mMain = main;			this.name1 = name;			this.previousTime = getTimer();			this.addEventListener(Event.ENTER_FRAME, OnEnterFrame);		}				protected function OnEnterFrame(e: Event): void {			var currentTime: int = getTimer();			if ((currentTime - this.previousTime) * 0.001 >= 3) {				var star: Star = new Star();				array.push(star);				star.x = Math.random() * 350 + 100;				star.y = 60;				var c:ColorTransform = new ColorTransform();				c.color = (Math.random() * 0xFFFFFF);				star.transform.colorTransform = c;				this.addChild(star);				this.previousTime = currentTime;			}			for (var i: int = 0; i < array.length; i++) {				if (array[i].click == true){					Score.text = String(parseInt(Score.text) + 1);					score++;					array[i].click = false;					}				if (array[i].y > 600){					Release();					this.removeChild(array[i]);					}			}		}				protected function OnCountDown(e: TimerEvent): void {}		override public function Release(): void {			this.removeEventListener(Event.ENTER_FRAME, OnEnterFrame);			this.addChild(new End(this.mMain, name1, score));		}	}}